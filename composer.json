{
    "name": "geniusee/money-hyb-geniusee-sdk",
    "type": "library",
    "authors": [
      {
        "name": "Alexander Bredikhin",
        "email": "alexbredikhin89@gmail.com"
      }
    ],
    "require": {
        "php": "^8.0",
        "firebase/php-jwt": "^5.5",
        "guzzlehttp/guzzle": "^7.4",
        "league/oauth2-client": "^2.6",
        "webmozart/assert": "^1.10",
        "laravel/helpers": "^1.5",
        "symfony/validator": "^6.0",
        "moneyphp/money": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "vimeo/psalm": "^4.12",
        "nunomaduro/phpinsights": "^2.0",
        "mockery/mockery": "^1.4",
        "friendsofphp/php-cs-fixer": "^3.4",
        "phpmetrics/phpmetrics": "^2.7",
        "dg/bypass-finals": "^1.3",
        "brainmaestro/composer-git-hooks": "^2.8"
    },
    "autoload": {
        "psr-4": {
            "Geniusee\\MoneyHubSdk\\": "src/"
        }
    },
    "autoload-dev": {
      "psr-4": {
        "Geniusee\\Tests\\": "tests/"
      }
    },
    "extra": {
      "laravel": {
        "dont-discover": []
      },
      "hooks": {
        "config": {
          "stop-on-failure": ["pre-push", "pre-commit"]
        },
        "pre-commit": ["make check-project"]
      }
    },
    "scripts": {
      "tests": "phpunit --testdox --no-interaction",
      "test-watch": "phpunit-watcher watch"
    }
}
